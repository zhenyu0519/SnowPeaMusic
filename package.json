{
  "name": "snowpeamusic",
  "version": "1.0.0",
  "engines": {
    "node": "10.18.x",
    "npm": "6.x"
  },
  "description": "Snow Pea Music based on the Spotify Music",
  "main": "server.js",
  "scripts": {
    "start": "cross-env node server.js",
    "server": "cross-env nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhenyu0519/SnowPeaMusic.git"
  },
  "author": "Jeffrey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhenyu0519/SnowPeaMusic/issues"
  },
  "homepage": "https://github.com/zhenyu0519/SnowPeaMusic#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "query-string": "^6.10.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^2.0.2"
  }
}
